<?xml version="1.0" encoding="utf-8" ?>
<Entity
  Name        = "StationLicenseInfo"
  Title       = "StationLicenseInfo"
  Desc        = "The station license info"
  Type        = "Normal"
  xmlns       = "http://schemas.atdi.com/orm/entity.xsd">
  <DataSource Type="Database" Object="Table">
    <Name>XBS_LICENCEINFO</Name>
    <Schema>dbo</Schema>
  </DataSource>
  <Fields>
    <Field Desc="Id" Title="Id" Name="Id" DataType="Counter.64" SourceName="ID" SourceType="Column"/>
    <Field Desc="License ID from ICSM" Title="License ID from ICSM" Name="IcsmId" DataType="Counter.64" SourceName="ICSMID" SourceType="Column"/>
    <Field Desc="License name" Title="License name" Name="Name" DataType="Text.100" SourceName="NAME" SourceType="Column" Required="false"/>
    <Field Desc="Start date" Title="Start date" Name="StartDate" DataType="DateTime" SourceName="STARTDATE" SourceType="Column" Required="false"/>
    <Field Desc="End Date" Title="End Date" Name="EndDate" DataType="DateTime" SourceName="ENDDATE" SourceType="Column" Required="false"/>
    <Field Desc="Close Date" Title="Close Date" Name="CloseDate" DataType="DateTime" SourceName="CLOSEDATE" SourceType="Column" Required="false"/>
  </Fields>

  <PrimaryKey Clustered="true">
    <FieldRef Name="Id"/>
  </PrimaryKey>
</Entity>