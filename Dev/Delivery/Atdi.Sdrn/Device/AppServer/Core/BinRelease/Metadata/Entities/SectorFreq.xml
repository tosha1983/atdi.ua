<?xml version="1.0" encoding="utf-8" ?>
<Entity
  Name        = "SectorFreq"
  Title       = "SectorFreq"
  Desc        = "The SectorFreq"
  Type        = "Normal"
  xmlns       = "http://schemas.atdi.com/orm/entity.xsd">
  <DataSource Type="Database" Object="Table">
    <Name>XBS_SECTORFREQ</Name>
    <Schema>dbo</Schema>
  </DataSource>
  <Fields>
    <Field Desc="Frequency identifier" Title="Frequency identifier" Name="Id" DataType="Counter.64" SourceName="ID" SourceType="Column"/>
    <Field Desc="Frequency plan identifier" Title="Frequency plan identifier" Name="PlanId" DataType="Integer.32" SourceName="IDPLAN" SourceType="Column" Required="true"/>
    <Field Desc="Channel number" Title="Channel number" Name="ChannelNumber" DataType="Integer.32" SourceName="CHANNALNUMBER" SourceType="Column" Required="true"/>
    <Field Desc="Frequency, MHz" Title="Frequency, MHz" Name="Frequency" DataType="Decimal.22.8" SourceName="FREQUENCY" SourceType="Column" Required="false"/>
    <Field Desc="IdSector" Title="IdSector" Name="IdSector" DataType="Integer.64" SourceName="ID_SECTOR" SourceType="Column"/>

    <Field Name="SECTOR" SourceType="Reference" SourceName="Sector" Title="Relation SectorFreq to Sector">
      <PrimaryKeyMapping>
        <Mapped KeyFieldName="Id" MatchWith="Field">IdSector</Mapped>
      </PrimaryKeyMapping>
    </Field>


  </Fields>
  <PrimaryKey Clustered="true">
    <FieldRef Name="Id"/>
  </PrimaryKey>
</Entity>