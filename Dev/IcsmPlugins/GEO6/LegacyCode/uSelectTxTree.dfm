  object sqlSelectTx: TIBSQL
    Database = dmMain.dbMain
    ParamCheck = True
    SQL.Strings = (
      
        'SELECT TX.ID, AR.NUMREGION, TX.ADMINISTRATIONID, SD.NAMESITE, TX' +
        '.LATITUDE, TX.LONGITUDE, SC.CODE SC_CODE, SC.ENUMVAL SC_ENUMVAL,' +
        ' CO.CODE CO_CODE, TX.VIDEO_CARRIER, CH.NAMECHANNEL, TX.SOUND_CAR' +
        'RIER_PRIMARY, TX.BLOCKCENTREFREQ, OW.NAMEORGANIZATION, BD.NAME B' +
        'D_NAME'
      'FROM TRANSMITTERS TX '
      
        '   LEFT OUTER JOIN ALLOTMENTBLOCKDAB AB ON (TX.ALLOTMENTBLOCKDAB' +
        '_ID = AB.ID)'
      '   LEFT OUTER JOIN BLOCKDAB BD ON (AB.BLOCKDAB_ID = BD.ID)'
      '   LEFT OUTER JOIN OWNER OW ON (TX.OWNER_ID = OW.ID)'
      '   LEFT OUTER JOIN SYSTEMCAST SC ON (TX.SYSTEMCAST_ID = SC.ID)'
      '   LEFT OUTER JOIN CHANNELS CH ON (TX.CHANNEL_ID = CH.ID)'
      '   LEFT OUTER JOIN STAND SD ON (TX.STAND_ID = SD.ID)'
      '   LEFT OUTER JOIN AREA AR ON (SD.AREA_ID = AR.ID)'
      '   LEFT OUTER JOIN COUNTRY CO ON (AR.COUNTRY_ID = CO.ID)'
      'WHERE '
      '   ('
      '      (TX.ID = :ID)'
      '   )'
      'ORDER BY AR.NUMREGION, TX.ADMINISTRATIONID')
    Transaction = dmMain.trMain
    Left = 72
    Top = 8
  end
