<?xml version="1.0" encoding="utf-8" ?>
<Entity
  Name        = "ResRoutes"
  Title       = "ResRoutes"
  Desc        = "The ResRoutes"
  Type        = "Normal"
  xmlns       = "http://schemas.atdi.com/orm/entity.xsd">
  <DataSource Type="Database" Object="Table">
    <Name>XBS_RESROUTES</Name>
    <Schema>ICSM</Schema>
  </DataSource>
  <Fields>
    <Field Desc="Id" Title="Id" Name="Id" DataType="Counter.64" SourceName="ID" SourceType="Column" Required="true"/>
    <Field Desc="RouteId" Title="RouteId" Name="RouteId" DataType="Text.250" SourceName="ROUTEID" SourceType="Column" Required="false"/>
    <Field Desc="Agl" Title="Agl" Name="Agl" DataType="Decimal.22.8" SourceName="AGL" SourceType="Column" Required="false"/>
    <Field Desc="Altitude above sea level, m" Title="Altitude above sea level, m" Name="Asl" DataType="Decimal.22.8" SourceName="ASL" SourceType="Column" Required="false"/>
    <Field Desc="StartTime" Title="StartTime" Name="StartTime" DataType="DateTime" SourceName="STARTTIME" SourceType="Column" Required="false"/>
    <Field Desc="FinishTime" Title="FinishTime" Name="FinishTime" DataType="DateTime" SourceName="FINISHTIME" SourceType="Column" Required="false"/>
    <Field Desc="Latitude, DEC" Title="Latitude, DEC" Name="Lat" DataType="Decimal.22.8" SourceName="LAT" SourceType="Column" Required="false" Unit="Latitude.DEC.xml"/>
    <Field Desc="Longitude, DEC" Title="Longitude, DEC" Name="Lon" DataType="Decimal.22.8" SourceName="LON" SourceType="Column" Required="false" Unit="Longitude.DEC.xml"/>
    <Field Desc="PointStayType" Title="PointStayType" Name="PointStayType" DataType="Text.150" SourceName="POINTSTAYTYPE" SourceType="Column" Required="false"/>
    <Field Desc="ResMeasId" Title="ResMeasId" Name="ResMeasId" DataType="Integer.64" SourceName="XBSRESMEASID" SourceType="Column" Required="false"/>
    <Field Name="RESMEAS" SourceType="Reference" SourceName="ResMeas" Title="Relation ResRoutes to ResMeas">
      <PrimaryKeyMapping>
        <Mapped KeyFieldName="Id" MatchWith="Field">ResMeasId</Mapped>
      </PrimaryKeyMapping>
    </Field>
  </Fields>
  <PrimaryKey Clustered="true">
    <FieldRef Name="Id"/>
  </PrimaryKey>
</Entity>