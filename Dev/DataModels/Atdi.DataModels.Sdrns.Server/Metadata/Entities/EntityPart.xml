<?xml version="1.0" encoding="utf-8" ?>
<Entity
  Name        = "EntityPart"
  Title       = "EntityPart"
  Desc        = "The EntityPart"
  Type        = "Normal"
  xmlns       = "http://schemas.atdi.com/orm/entity.xsd">
  <DataSource Type="Database" Object="Table">
    <Name>XBS_ENTITYPART</Name>
    <Schema>ICSM</Schema>
  </DataSource>
  <Fields>
    <Field Desc="EntityId" Title="Id" Name="Id" DataType="Text.250" SourceName="ENTITYID" SourceType="Column" Required="true"/>
    <Field Desc="PartIndex" Title="PartIndex" Name="PartIndex" DataType="Integer.32" SourceName="PARTINDEX" SourceType="Column" Required="true"/>
    <Field Desc="Eof" Title="Eof" Name="Eof" DataType="Integer.64" SourceName="EOF" SourceType="Column" Required="true"/>
    <Field Desc="Content" Title="Content" Name="Content" DataType="Bytes.MAX" SourceName="CONTENT" SourceType="Column" Required="true"/>
    <Field Name="ENTITY" SourceType="Reference" SourceName="Entity" Title="Relation EntityPart to Entity">
      <PrimaryKeyMapping>
        <Mapped KeyFieldName="Id" MatchWith="Field">EntityId</Mapped>
      </PrimaryKeyMapping>
    </Field>
  </Fields>
  <PrimaryKey Clustered="true">
    <FieldRef Name="EntityId"/>
    <FieldRef Name="PartIndex"/>
  </PrimaryKey>
</Entity>