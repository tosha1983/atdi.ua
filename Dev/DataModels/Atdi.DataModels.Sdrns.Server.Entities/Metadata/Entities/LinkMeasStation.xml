<?xml version="1.0" encoding="utf-8" ?>
<Entity
  Name        = "LinkMeasStation"
  Title       = "LinkMeasStation"
  Desc        = "The LinkMeasStation"
  Type        = "Normal"
  xmlns       = "http://schemas.atdi.com/orm/entity.xsd">
