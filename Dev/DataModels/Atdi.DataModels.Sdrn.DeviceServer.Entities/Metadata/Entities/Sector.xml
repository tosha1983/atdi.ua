<?xml version="1.0" encoding="utf-8" ?>
<Entity
  Name        = "Sector"
  Title       = "Sector"
  Desc        = "The Sector"
  Type        = "Normal"
  xmlns       = "http://schemas.atdi.com/orm/entity.xsd">
  <DataSource Type="Database" Object="Table">
    <Name>XBS_SECTOR</Name>
    <Schema>dbo</Schema>
  </DataSource>
  <Fields>
    <Field Desc="Id" Title="Id" Name="Id" DataType="Counter.64" SourceName="ID" SourceType="Column"/>
    <Field Desc="Sector ID from ICSM" Title="Sector ID from ICSM" Name="SectorId" DataType="Text.100" SourceName="IDSECTOR" SourceType="Column" Required="true"/>
    <Field Desc="Altitude above ground level, m" Title="Altitude above ground level, m" Name="Agl" DataType="Decimal.22.8" SourceName="AGL" SourceType="Column" Required="false"/>
    <Field Desc="EIRP, dBm" Title="Eirp" Name="EIRP, dBm" DataType="Decimal.22.8" SourceName="EIRP" SourceType="Column" Required="false"/>
    <Field Desc="Azimuth, degrees" Title="Azimuth, degrees" Name="Azimut" DataType="Decimal.22.8" SourceName="AZIMUT" SourceType="Column" Required="false"/>
    <Field Desc="Bandwidth, kHz" Title="Bandwidth, kHz" Name="Bw" DataType="Decimal.22.8" SourceName="BW" SourceType="Column" Required="false"/>
    <Field Desc="Class of emission" Title="Class of emission" Name="ClassEmission" DataType="Text.20" SourceName="CLASSEMISSION" SourceType="Column" Required="false"/>
    <Field Desc="StationId" Title="StationId" Name="StationId" DataType="Integer.64" SourceName="ID_MEASSTATION" SourceType="Column" Required="true"/>

    <Field Name="STATION" SourceType="Reference" SourceName="MeasStation" Title="Relation Sector to MeasStation">
      <PrimaryKeyMapping>
        <Mapped KeyFieldName="Id" MatchWith="Field">StationId</Mapped>
      </PrimaryKeyMapping>
    </Field>
  </Fields>
  <PrimaryKey Clustered="true">
    <FieldRef Name="Id"/>
  </PrimaryKey>
</Entity>