<?xml version="1.0" encoding="utf-8" ?>
<Entity 
  Name        = "ResultsLocation" 
  Title       = "ResultsLocation" 
  Desc        = "The ResultsLocation"
  Type        = "Normal"
  xmlns       = "http://schemas.atdi.com/orm/entity.xsd">
  <DataSource Type="Database" Object="Table">
    <Name>XBS_RESULTSLOCATION</Name>
    <Schema>dbo</Schema>
  </DataSource>
  <Fields>
    <Field Desc="Id" Title="Id" Name="Id" DataType="Counter.64" SourceName="ID" SourceType="Column" Required="true"/>
    <Field Desc="Longitude, DEC" Title="Longitude, DEC" Name="Lon" DataType="Decimal.22.8" SourceName="LON" SourceType="Column" Required="false" Unit="Longitude.DEC"/>
    <Field Desc="Latitude, DEC" Title="Latitude, DEC" Name="Lat" DataType="Decimal.22.8" SourceName="LAT" SourceType="Column" Required="false" Unit="Latitude.DEC"/>
    <Field Desc="Altitude above sea level, m" Title="Altitude above sea level, m" Name="Asl" DataType="Decimal.22.8" SourceName="ASL" SourceType="Column" Required="false"/>
    <Field Desc="IdMeasSdrResults" Title="IdMeasSdrResults" Name="IdMeasSdrResults" DataType="Integer.64" SourceName="IDMEASSDRRESULTS" SourceType="Column" Required="true"/>
    <Field Name="MEASSDRRESULTS" SourceType="Reference" SourceName="MeasSDRResults" Title="Relation ResultsLocation to MeasSDRResults">
      <PrimaryKeyMapping>
        <Mapped KeyFieldName="Id" MatchWith="Field">IdMeasSdrResults</Mapped>
      </PrimaryKeyMapping>
    </Field>
  </Fields>
  <PrimaryKey Clustered="true">
    <FieldRef Name="Id"/>
  </PrimaryKey>
</Entity>

