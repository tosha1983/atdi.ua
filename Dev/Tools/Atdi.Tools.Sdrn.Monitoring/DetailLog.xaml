<Window x:Class="Atdi.Tools.Sdrn.Monitoring.DetailLog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Atdi.Tools.Sdrn.Monitoring"
        mc:Ignorable="d"
        Title="SDRN Servers Monitoring - Log Detail" Height="450" Width="800" WindowState="Maximized">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*" />
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <TreeView x:Name="mainTree" Grid.Column="0" Grid.Row="0" Margin="5"/>
    </Grid>
</Window>
