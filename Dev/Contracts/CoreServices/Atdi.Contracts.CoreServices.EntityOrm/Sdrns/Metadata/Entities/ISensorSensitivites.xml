<?xml version="1.0" encoding="utf-8" ?>
<Entity 
  Name        = "ISensorSensitivites" 
  Title       = "SensorSensitivites" 
  Desc        = "The sensor equipment sensitivites"
  Type        = "Normal"
  xmlns       = "http://schemas.atdi.com/orm/entity.xsd">
  
  <DataSource Type="Database" Object="Table">
    <Name>XBS_SENSOREQUIPSENS</Name>
    <Schema>ICSM</Schema>
  </DataSource>
  
  <Fields>
    <Field Name="Id" 
     SourceType="Column" 
     SourceName="ID" 
     DataType="Counter" 
     Title="Id" 
     Desc="Id"/>

    <Field Name="SensorEquipId"
     SourceType="Column"
     SourceName="SENSOREQUIP_ID"
     DataType="Int64"
     Required="false"
     Title="SensorEquipId"
     Desc="SensorEquipId"/>

   
    <Field Name="Freq"
      SourceType="Column"
      SourceName="FREQ"
      DataType="Double"
      Required="false"
      Title="Freq, MHz"
      Desc="Freq, MHz"
      Unit="Frequency.MHz"/>

    <Field Name="Ktbf"
      SourceType="Column"
      SourceName="KTBF"
      DataType="Double"
      Required="false"
      Title="own noise level, dBm"
      Desc="own noise level, dBm"
      Unit="Level.dBm"/>

    <Field Name="Noisef"
      SourceType="Column"
      SourceName="NOISEF"
      DataType="Double"
      Required="false"
      Title=" noise figure, dB"
      Desc=" noise figure, dB"
      Unit="Power.dB"/>

    <Field Name="FreqStability"
     SourceType="Column"
     SourceName="FREQSTABILITY"
     DataType="Double"
     Required="false"
     Title="FreqStability, %"
     Desc="FreqStability, %"
     Unit="Frequency.MHz"/>


    <Field Name="AddLoss"
     SourceType="Column"
     SourceName="ADDLOSS"
     DataType="Double"
     Required="false"
     Title="Additional Loss, dB"
     Desc="Additional Loss, dB"
     Unit="Power.dB"/>

    <Field
    Name="SENSOREQUIP"
    SourceType="Reference"
    SourceName ="ISensorEquipment"
    Title="Relation SensorEquipmentSensitivites to SensorEquipment" >
      <PrimaryKeyMapping>
        <Mapped KeyFieldName="Id" MatchWith="Field">SensorEquipId</Mapped>
      </PrimaryKeyMapping>
    </Field>
    
    
  </Fields>


  <PrimaryKey Clustered="true">
    <FieldRef Name="Id" SortOrder="Descending" />
  </PrimaryKey>

  </Entity>