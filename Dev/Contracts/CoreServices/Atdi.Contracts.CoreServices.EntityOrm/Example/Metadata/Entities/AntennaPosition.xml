<?xml version="1.0" encoding="utf-8" ?>

<Entity xmlns = "http://schemas.atdi.com/orm/entity.xsd"
        Name  = "AntennaPosition"
        Title = "Antenna position entity"
        Desc  = "The antenna position entity"
        Type  = "Normal"
        >
  <DataSource Type="Database" Object="Table">
    <Name>ANTENNA_POS</Name>
    <Schema>Eqip</Schema>
  </DataSource>

  <Fields>
    <Field
      Name="Id"
      SourceType="Column"
      SourceName="ID"
      DataType="Counter"
      Required="true"
      Title="Pos ID"/>

    <Field
      Name="TableName"
      SourceType="Column"
      SourceName ="TABLE_NAME"
      DataType="Text.50"
      Required="true"
      Title="Table Name" />

    <Field
      Name="PosType"
      SourceType="Column"
      SourceName ="POS_TYPE"
      DataType="Text.50"
      Required="true"
      Title="Table Name" />

    <Field
      Name="PosX"
      SourceType="Column"
      SourceName ="POS_X"
      Required="false"
      DataType="Number.28.10"
      Title="Pos X" />
    <Field
      Name="PosY"
      SourceType="Column"
      SourceName ="POS_Y"
      Required="false"
      DataType="Number.28.10"
      Title="Pos X" />
  </Fields>

  <PrimaryKey Clustered="true">
    <FieldRef Name="Id"/>
    <FieldRef Name="TableName"/>
    <FieldRef Name="PosType"/>
  </PrimaryKey>

</Entity>