<?xml version="1.0" encoding="utf-8" ?>

<Entity xmlns = "http://schemas.atdi.com/orm/entity.xsd"
        Name  = "AntennaBase"
        Title = "Antenna base entity"
        Desc  = "The antenna base entity"
        Type  = "Normal"
        >
  <DataSource Type="Database" Object="Table">
    <Name>ANTENNA_BASE</Name>
    <Schema>dbo</Schema>
  </DataSource>
  
  <Fields>
    <Field 
      Name="Id"   
      SourceType="Column"    
      SourceName="ID"     
      DataType="Counter"   
      Required="true"
      Title="Antenna ID"/>
    
    <Field 
      Name="Name" 
      SourceType="Column"    
      SourceName ="NAME"  
      DataType="Text.100"
      Required="true"
      Title="Antenna Name" />
    
    <Field 
      Name="TYPE" 
      SourceType="Reference" 
      SourceName ="Atdi.DataModels.SDRN.Server.Types.AntennaType" 
      Title="Antenna type" />
      
  </Fields>

  <PrimaryKey Clustered="true">
    <FieldRef Name="Id"/>
  </PrimaryKey>
  
</Entity>