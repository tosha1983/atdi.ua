<?xml version="1.0" encoding="utf-8" ?>
<Entity 
  Name        = "SensorEquipment" 
  Title       = "SensorEquipment" 
  Desc        = "The sensor equipment"
  Type        = "Normal"
  xmlns       = "http://schemas.atdi.com/orm/entity.xsd">
  
  <DataSource Type="Database" Object="Table">
    <Name>XBS_SENSOREQUIP</Name>
    <Schema>ICSM</Schema>
  </DataSource>
  
  <Fields>
    <Field Name="Id" 
     SourceType="Column" 
     SourceName="ID" 
     DataType="Counter" 
     Title="Id" 
     Desc="Id"/>

    <Field Name="SensorId"
     SourceType="Column"
     SourceName="SENSORID"
     DataType="Int.15.0"
     Required="true"
     Title="SensorId"
     Desc="SensorId"/>

    <Field Name="Code"
      SourceType="Column"
      SourceName="CODE"
      DataType="Text.50"
      Required="false"
      Title="Code of equipment"
      Desc="Code of equipment"/>


    <Field Name="Manufacturer"
     SourceType="Column"
     SourceName="MANUFACTURER"
     DataType="Text.50"
     Required="false"
     Title="Manufacturer"
     Desc="Manufacturer"/>

    <Field Name="Name"
    SourceType="Column"
    SourceName="NAME"
    DataType="Text.50"
    Required="false"
    Title="Name"
    Desc="Name"/>


    <Field Name="Family"
    SourceType="Column"
    SourceName="FAMILY"
    DataType="Text.50"
    Required="false"
    Title="Family"
    Desc="Family"/>
    
    <Field Name="TechId"
    SourceType="Column"
    SourceName="TECHID"
    DataType="Text.200"
    Required="false"
    Title="Technical ID"
    Desc="Technical ID"/>

    <Field Name="Version"
    SourceType="Column"
    SourceName="VERSION"
    DataType="Text.50"
    Required="false"
    Title="Version"
    Desc="Version"/>

  <Field Name="LowerFreq"
   SourceType="Column"
   SourceName="LOWERFREQ"
   DataType="Double.22.8"
   Required="false"
   Title="LowerFreq, MHz"
   Desc="LowerFreq, MHz"
   Unit="Frequency.MHz"/>


  <Field Name="UpperFreq"
   SourceType="Column"
   SourceName="UPPERFREQ"
   DataType="Double.22.8"
   Required="false"
   Title="UpperFreq, MHz "
   Desc="UpperFreq, MHz "
   Unit="Frequency.MHz"/>


   <Field Name="RbwMin"
   SourceType="Column"
   SourceName="RBWMIN"
   DataType="Double.22.8"
   Required="false"
   Title="RBWMin, kHz"
   Desc="RBWMin, kHz"
   Unit="Frequency.kHz"/>

   <Field Name="RbwMax"
    SourceType="Column"
    SourceName="RBWMAX"
    DataType="Double.22.8"
    Required="false"
    Title="RBWMax, kHz"
    Desc="RBWMax, kHz"
    Unit="Frequency.kHz"/>

    <Field Name="VbwMin"
    SourceType="Column"
    SourceName="VBWMIN"
    DataType="Double.22.8"
    Required="false"
    Title="VBWMin, kHz"
    Desc="VBWMin, kHz"
    Unit="Frequency.kHz"/>

    <Field Name="VbwMax"
    SourceType="Column"
    SourceName="VBWMAX"
    DataType="Double.22."
    Required="false"
    Title="VBWMax, kHz"
    Desc="VBWMax, kHz"
    Unit="Frequency.kHz"/>

    <Field Name="Mobility"
    SourceType="Column"
    SourceName="MOBILITY"
    DataType="Number.1.0"
    Required="false"
    Title="Mobility - true - can be mobile, false cannot be mobile"
    Desc="Mobility - true - can be mobile, false cannot be mobile"/>

    <Field Name="FftPointMax"
    SourceType="Column"
    SourceName="FFTPOINTMAX"
    DataType="Double.22.8"
    Required="false"
    Title="Maximum of points for Fast Fourier Transform"
    Desc="Maximum of points for Fast Fourier Transform"/>


    <Field Name="RefLevelDbm"
    SourceType="Column"
    SourceName="REFLEVELDBM"
    DataType="Double.22.8"
    Required="false"
    Title="Reference level in dBm"
    Desc="Reference level in dBm"
    Unit="Level.dBm"/>
    
 
    <Field Name="OperationMode"
    SourceType="Column"
    SourceName="OPERATIONMODE"
    DataType="Text.50"
    Required="false"
    Title="OperationMode"
    Desc="OperationMode"/>

    <Field Name="Type"
    SourceType="Column"
    SourceName="TYPE"
    DataType="Text.50"
    Required="false"
    Title="Type"
    Desc="Type"/>

    <Field Name="EquipClass"
    SourceType="Column"
    SourceName="EQUIPCLASS"
    DataType="Text.50"
    Required="false"
    Title="EquipClass"
    Desc="EquipClass"/>


    <Field Name="TuningStep"
    SourceType="Column"
    SourceName="TUNINGSTEP"
    DataType="Double.22.8"
    Required="false"
    Title="TuningStep, Hz"
    Desc="TuningStep, Hz"
    Unit="Frequency.Hz"/>

    <Field Name="UserType"
    SourceType="Column"
    SourceName="USETYPE"
    DataType="Text.50"
    Required="false"
    Title="UserType"
    Desc="UserType"/>

    <Field Name="Category"
    SourceType="Column"
    SourceName="CATEGORY"
    DataType="Text.50"
    Required="false"
    Title="Category"
    Desc="Category"/>

    <Field Name="Remark"
    SourceType="Column"
    SourceName="REMARK"
    DataType="Text.250"
    Required="false"
    Title="Remark"
    Desc="Remark"/>

    <Field Name="CustTxt1"
    SourceType="Column"
    SourceName="CUSTTXT1"
    DataType="Text.250"
    Required="false"
    Title="CustTxt1"
    Desc="CustTxt1"/>

    <Field Name="CustData1"
    SourceType="Column"
    SourceName="CUSTDATA1"
    DataType="DateTime"
    Required="false"
    Title="CustData1"
    Desc="CustData1"/>


    <Field Name="CustNbr1"
    SourceType="Column"
    SourceName="CUSTNBR1"
    DataType="Double.22.8"
    Required="false"
    Title="CustNbr1"
    Desc="CustNbr1"/>
    


    <Field Name="SENSOR"
         SourceType="Reference"
         SourceName="Sensor"
         Title="Relation SensorEquipment to Sensor" >
      <PrimaryKeyMapping>
        <Map SourceName="Id" This="SensorId" />
      </PrimaryKeyMapping>
    </Field>
    
  </Fields>


  <PrimaryKey Clustered="true">
    <FieldRef Name="Id" SortOrder="Descending" />
  </PrimaryKey>

  </Entity>